                                </div>
                                
                                <div data-v-fc8649f4="">
                                    <div data-v-fc8649f4="" id="deposit-amount" class="num">${depositAmount}</div>
                                    <div data-v-fc8649f4="">Deposit amount</div>
                                </div>
                                <div data-v-fc8649f4="" style="border-right: 1px solid black;">
                                    <div data-v-fc8649f4="" id="bet-num" class="num">${betNum}</div>
                                    <div data-v-fc8649f4="">Number of bettors</div>
                                </div>
                                <div data-v-fc8649f4="">
                                    <div data-v-fc8649f4="" id="bet-amount" class="num">${betAmount}</div>
                                    <div data-v-fc8649f4="">Total bet</div>
                                </div>
                                <div data-v-fc8649f4="" style="border-right: 1px solid black;">
                                    <div data-v-fc8649f4="" class="num">${firstRecharge}</div>
                                    <div data-v-fc8649f4="">Number of people making first deposit</div>
                                </div>
                                <div data-v-fc8649f4="">
                                    <div data-v-fc8649f4="" class="num">${firstRechargeAmount}</div>
                                    <div data-v-fc8649f4="">First deposit amount</div>
                                </div>
                            </div>
                        </div>`;
                    datas.map((data) => {
                        html += `
                        <div data-v-a8993d41="" class="rechargeh__container-content__item">
                <div data-v-a8993d41="" class="rechargeh__container-content__item-header ar-1px-b">
                    <span data-v-a8993d41="" van-image" data-clipboard-text="${data.id_user}" class="copy_clip" class="btn van-button--default">UID:${data.id_user}<img style="width: 18px; height: 15px;" src="/images/copy.png"></span>
                    <span data-v-a8993d41="" class=""> </span>
                </div>

                <div data-v-a8993d41="" class="rechargeh__container-content__item-body">
                    <div data-v-a8993d41="">
                        <span data-v-a8993d41="" class="withdraw_title">Level</span>
                        <span data-v-a8993d41="" class="balance_red">${data.user_level}</span>
                    </div>

                    <div data-v-a8993d41="">
                        <span data-v-a8993d41="">Deposit amount</span>
                        <span data-v-a8993d41=""> ${data.deposit_count}</span>
                    </div>

                    <div data-v-a8993d41="">
                        <span data-v-a8993d41="">Bet Amount</span>
                        <span data-v-a8993d41="">${data.bet_amount}</span>
                    </div>

                    <div data-v-a8993d41="" >
                        <span data-v-a8993d41="">Commision</span>
                        <span data-v-a8993d41="" class="">${data.comm_amount}</span>
                    </div>

                    <div data-v-a8993d41="" >
                        <span data-v-a8993d41="">Time</span>
                        <span data-v-a8993d41="">${mydate}</span>
                    </div>
                </div>
            </div> 
                        `;
                    });
                    $('#van-list').show();
                    $('#van-list').html(html);

                    $('.copy_clip').click(function (e) {
                        e.preventDefault();
                        let text = $(this).text().trim();
                        navigator.clipboard.writeText(text);
                        $(this).addClass('block-click');
                        alertMess('Copy successful', $(this));
                    });
                    
                    $('.showListButton').on('click', function () {
                        const inviteCount = $(this).data('invite-count');
                        const listHTML = Array.isArray(inviteCount) ? `<ul>${inviteCount.map(item => `<li>${item}</li>`).join('')}</ul>` : 'No Subordinates';
                        $('#subordinateList').html(`Subordinate: ${listHTML}`);
                    });

                }
                
                var d = new Date();
                var today = formateT(d.getFullYear()) + "-" + formateT((d.getMonth()+1)) + "-" + formateT(d.getDate()-1);
              
                $.ajax({
                    type: "POST",
                    url: "/api/webapi/myTeam",
                    data: {
                        todate: today,
                    },
                    dataType: "json",
                    success: function (response) {
                        //console.log('user name is :' + response.f1);
                        const f1Copy = [...response.f1];
                        f1Copy.splice(0, 4);
                        const mydate = response.mydate;
                        //console.log(response.deposit_num);
                        const f1Data = response.f1.map(item => {
                            item.daily_turn_over = item.daily_turn_over || 0;
                            item.invite_count = Array.isArray(item.invite_count) ? item.invite_count.join(', ') : '';
                            item.deposit_count = item.deposit_count || 0;
                            item.deposit_num = item.deposit_num || 0;
                            item.first_recharge = item.first_recharge || 0;
                            item.first_recharge_amount = item.first_recharge_amount || 0;
                            item.bet_amount  = item.bet_amount == null ? 0: parseInt(item.bet_amount,10);
                            item.comm_amount = item.comm_amount || 0;
                            item.total_turn_over = item.total_turn_over || 0;
                            //console.log(item.first_recharge);
                            return item;
                        });
                        const totalTurnover = f1Data.reduce((total, item) => {
                            const turnover = parseFloat(item.deposit_num);
                            return isNaN(turnover) ? total : total + turnover;
                        }, 0);

                        const depositAmount = f1Data.reduce((total, item) => {
                            const depositamount = parseFloat(item.deposit_count);
                            return isNaN(depositamount) ? total : total + depositamount;
                        }, 0);
                      
                        const betAmount = f1Data.reduce((total, item) => {
                            const betamount = parseFloat(item.bet_amount);
                            return isNaN(betamount) ? total : total + betamount;
                        }, 0);
                      
                        const depositNum = f1Data.reduce((total, item) => {
                            const depositnum = parseFloat(item.deposit_num);
                            return isNaN(depositnum) ? total : total + depositnum;
                        }, 0);

                        const betNum = f1Data.reduce((total, item) => {
                            const betnum = parseFloat(item.bet_num);
                            return isNaN(betnum) ? total : total + betnum;
                        }, 0);

                        const firstRecharge = f1Data.reduce((total, item) => {
                            const firstrechage = parseFloat(item.first_recharge);
                            return isNaN(firstrechage) ? total : total + firstrechage;
                        }, 0);
                        
                        const firstRechargeAmount = f1Data.reduce((total, item) => {
                            const firstrechageamount = parseFloat(item.first_recharge_amount);
                            return isNaN(firstrechageamount) ? total : total + firstrechageamount;
                        }, 0);
                       
                        const totalMoneySum = response.f1.reduce((total, user) => {
                            const money = parseFloat(user.total_money) || 0;
                            return total + money;
                        }, 0);
                        
                       // console.log(count(depositNum));
                        
                        
                        $('#total-mem').text(`Total Recharge: ${totalMoneySum} | Total Turnover: ${parseFloat(totalTurnover)}`);
                        MemRender(f1Data, mydate, depositAmount, betAmount, depositNum, betNum, firstRecharge, firstRechargeAmount);
                    }
                });
            </script>
            <script>

                function filterData(uid, level, mydate) {
                    $.ajax({
                        type: "POST",
                        url: "/api/webapi/myTeam",
                        data: {
                        todate: mydate,
                        },
                        dataType: "json",
                        beforeSend: function() {
                            $('#van-list').hide();
                        },
                        success: function (response) {
                            const mydate = response.mydate;
                            const allData = response.f1;
                            const filteredData = allData.filter(item => {
                                const uidMatch = uid === "" || item.id_user.includes(uid);
                                const levelMatch = level === "" || parseInt(item.user_level) === parseInt(level);
                                const mydate = response.mydate;
                                return uidMatch && levelMatch && mydate;
                            });

                            const depositAmount = filteredData.reduce((total, item) => {
                            const depositamount = parseFloat(item.deposit_count);
                            return isNaN(depositamount) ? total : total + depositamount;
                            }, 0);

                            const betAmount = filteredData.reduce((total, item) => {
                            const betamount = parseFloat(item.bet_amount);
                            return isNaN(betamount) ? total : total + betamount;
                            }, 0);

                            const depositNum = filteredData.reduce((total, item) => {
                            const depositnum = parseFloat(item.deposit_num);
                            return isNaN(depositnum) ? total : total + depositnum;
                            }, 0);

                            const betNum = filteredData.reduce((total, item) => {
                            const betnum = parseFloat(item.bet_num);
                            return isNaN(betnum) ? total : total + betnum;
                            }, 0);

                            const firstRecharge = filteredData.reduce((total, item) => {
                            const firstrechage = parseFloat(item.first_recharge);
                            return isNaN(firstrechage) ? total : total + firstrechage;
                            }, 0);
                            
                            const firstRechargeAmount = filteredData.reduce((total, item) => {
                            const firstrechageamount = parseFloat(item.first_recharge_amount);
                            return isNaN(firstrechageamount) ? total : total + firstrechageamount;
                            }, 0);
                       

                            if (filteredData.length > 0) {
                                let html = '';
                                html +=`<div data-v-fc8649f4="" class="TeamReport__C-body">
                            <div data-v-fc8649f4="" class="header-container">
                                <div data-v-fc8649f4="" style="border-right: 1px solid black;">
                                    <div data-v-fc8649f4="" id="deposit-num" class="num">${depositNum}</div>
                                    <div data-v-fc8649f4="">Deposit number</div>
                                </div>
                                
                                <div data-v-fc8649f4="">
                                    <div data-v-fc8649f4="" id="deposit-amount" class="num">${depositAmount}</div>
                                    <div data-v-fc8649f4="">Deposit amount</div>
                                </div>
                                <div data-v-fc8649f4="" style="border-right: 1px solid black;">
                                    <div data-v-fc8649f4="" id="bet-num" class="num">${betNum}</div>
                                    <div data-v-fc8649f4="">Number of bettors</div>
                                </div>
                                <div data-v-fc8649f4="">
                                    <div data-v-fc8649f4="" id="bet-amount" class="num">${betAmount}</div>
                                    <div data-v-fc8649f4="">Total bet</div>
                                </div>
                                <div data-v-fc8649f4="" style="border-right: 1px solid black;">
                                    <div data-v-fc8649f4="" class="num">${firstRecharge}</div>
                                    <div data-v-fc8649f4="">Number of people making first deposit</div>
                                </div>
                                <div data-v-fc8649f4="">
                                    <div data-v-fc8649f4="" class="num">${firstRechargeAmount}</div>
                                    <div data-v-fc8649f4="">First deposit amount</div>
                                </div>
                            </div>
                        </div>`;
                                filteredData.forEach(data => {
                                    html += `
                                    <div data-v-a8993d41="" class="rechargeh__container-content__item">
                <div data-v-a8993d41="" class="rechargeh__container-content__item-header ar-1px-b">
                    <span data-v-a8993d41="" van-image" data-clipboard-text="${data.id_user}" class="copy_clip" class="btn van-button--default">UID:${data.id_user}<img style="width: 18px; height: 15px;" src="/images/copy.png"></span>
                    <span data-v-a8993d41="" class=""> </span>
                </div>

                <div data-v-a8993d41="" class="rechargeh__container-content__item-body">
                    <div data-v-a8993d41="">
                        <span data-v-a8993d41="" class="withdraw_title">Level</span>
                        <span data-v-a8993d41="" class="balance_red">${data.user_level}</span>
                    </div>

                    <div data-v-a8993d41="">
                        <span data-v-a8993d41="">Deposit amount</span>
                        <span data-v-a8993d41="">${data.deposit_count == null ? 0: parseInt(data.deposit_count)}</span>
                    </div>

                    <div data-v-a8993d41="">
                        <span data-v-a8993d41="">Bet Amount</span>
                        <span data-v-a8993d41="">${data.bet_amount == null ? 0: parseInt(data.bet_amount)}</span>
                    </div>

                    <div data-v-a8993d41="" >
                        <span data-v-a8993d41="">Commision</span>
                        <span data-v-a8993d41="" class="">${data.comm_amount == null ? 0: parseInt(data.comm_amount,10)}</span>
                    </div>

                    <div data-v-a8993d41="" >
                        <span data-v-a8993d41="">Time</span>
                        <span data-v-a8993d41="">${mydate}</span>
                    </div>
                </div>
            </div> 
                    `;
                                });
                                $('#van-list').show();
                                $('#van-list').html(html);
            
                                $('.copy_clip').click(function (e) {
                                    e.preventDefault();
                                    let text = $(this).text().trim();
                                    navigator.clipboard.writeText(text);
                                    $(this).addClass('block-click');
                                    alertMess('Copy successful', $(this));
                                });
                                
                                $('.showListButton').on('click', function () {
                                    const inviteCount = $(this).data('invite-count');
                                    const listHTML = Array.isArray(inviteCount) ? `<ul>${inviteCount.map(item => `<li>${item}</li>`).join('')}</ul>` : 'No Subordinates';
                                    $('#subordinateList').html(`Subordinate: ${listHTML}`);
                                });
                            }
                        }
                    });
                }

                $('.iptuid').on('input', function () {
                    const partialUID = $(this).val().trim();
                    const selectedLevel = $('#levelSelect').val();
                    const partialDate = $('.iptdate').val();
                    filterData(partialUID, selectedLevel, partialDate);
                });

                $('.iptdate').on('change', function () {
                    const partialDate = $(this).val();
                    const partialUID = $('.iptuid').val().trim();
                    const selectedLevel = $('#levelSelect').val();
                    filterData(partialUID, selectedLevel, partialDate);
                   
                });

                $('#levelSelect').on('change', function () {
                    const selectedLevel = $(this).val();
                    const partialUID = $('.iptuid').val().trim();
                    const partialDate = $('.iptdate').val();
                    filterData(partialUID, selectedLevel, partialDate);
                });
                filterData("", "");

            </script>

</body>

